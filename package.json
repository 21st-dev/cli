{
  "name": "@21st-dev/cli",
  "version": "0.0.2",
  "type": "module",
  "description": "MCP configuration installer by 21st.dev",
  "homepage": "https://21st.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/21st-dev/cli.git"
  },
  "bin": {
    "mcp-install": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai",
    "21st.dev",
    "21",
    "cli",
    "installer"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && shx chmod +x dist/*.js",
    "start": "node dist/index.js",
    "prepare": "npm run build",
    "dev": "nodemon --watch src --ext ts,json --exec \"npm run build\"",
    "publish-patch": "npm version patch && npm run build && npm publish --access public"
  },
  "author": "sergeibynas@gmail.com",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "inquirer": "^8.2.5",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.5",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "jest": "^29.0.0",
    "nodemon": "^3.1.9",
    "shx": "^0.3.4",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
